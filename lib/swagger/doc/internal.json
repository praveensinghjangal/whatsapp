                        },
                        "msg": {
                           "type": "string"
                        },
                        "data": {
                           "type": "object"
                        },
                        "error": {
                           "type": "array"
                        }
                     },
                     "example": {
                        "code": 4000,
                        "msg": "Invalid request",
                        "data": null,
                        "error": [
                           "invalid body error here"
                        ]
                     }
