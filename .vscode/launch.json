  "version": "0.2.0",
  "configurations": [
  

    {
      "command": "npm run develop",
      "name": "Run npm run develop",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9231,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/server.js",
      "console": "integratedTerminal"
    }
  ]
}