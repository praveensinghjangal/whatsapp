{
  "name": "helo-whatsapp",
  "description": "This app contains whatsapp platform for vivaconnect",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "develop": "nodemon server.js",
    "jsodoc": "./node_modules/.bin/jsdoc -c jsdoc.conf -d public/js-docs",
    "debug": "nodemon --inspect=9231 server.js",
    "v8Profile": "node --prof server.js",
    "standard": "standard --fix"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "watch": [
      "app_modules",
      "routes"
    ],
    "events": {
      "restart": "fuser -k 3000/tcp;",
      "start": "fuser -k 3000/tcp;",
      "crash": "fuser -k 3000/tcp;"
    },
    "delay": "1500"
  },
  "pre-commit": [
    "standard"
  ],
  "standard": {
    "ignore": [
      "/lib/"
    ]
  },
  "dependencies": {
    "amqplib": "0.5.5",
    "api-responses": "git+http://danish.galiyara:NHaBKpsXVkrkQKmUxest@git.vivaconnect.co/helo-whatsapp/api-responses.git#1.0.6",
    "async": "3.1.1",
    "aws-sdk": "2.682.0",
    "bottleneck": "^2.19.5",
    "connect-timeout": "1.9.0",
    "convert-excel-to-json": "1.7.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "country-state-city": "1.0.4",
    "date-format-utils": "2.2.0",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-request-id": "1.4.1",
    "express-validator": "6.1.1",
    "fs": "0.0.1-security",
    "hbs": "4.0.2",
    "helmet": "3.20.0",
    "http-errors": "1.7.3",
    "humanize-plus": "1.8.2",
    "jsdoc": "^3.6.6",
    "jsdoc-http-plugin": "^0.3.2",
    "json2csv": "3.11.2",
    "jsonschema": "1.2.6",
    "jsonwebtoken": "8.5.1",
    "jwt-simple": "0.5.5",
    "line-by-line": "0.1.6",
    "lodash": "4.17.15",
    "moment": "2.25.3",
    "moment-timezone": "0.5.26",
    "mongodb": "3.1.12",
    "mongodb-autoincrement": "1.0.1",
    "morgan": "1.9.0",
    "multer": "1.4.2",
    "multer-s3": "2.9.0",
    "mysql2": "2.1.0",
    "node-cron": "^2.0.3",
    "node-salesforce": "0.8.0",
    "nodemailer": "6.4.6",
    "nodemailer-smtp-transport": "2.7.4",
    "passport": "0.4.0",
    "passport-facebook": "3.0.0",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "path": "^0.12.7",
    "pg": "8.1.0",
    "qrcode": "1.4.4",
    "rate-limiter-flexible": "2.1.10",
    "redis": "2.8.0",
    "request": "2.88.2",
    "serve-favicon": "2.5.0",
    "socket.io": "2.2.0",
    "speakeasy": "2.0.0",
    "uuid4": "1.1.4",
    "viva-logger": "git+http://danish.galiyara:NHaBKpsXVkrkQKmUxest@git.vivaconnect.co/helo-whatsapp/viva-logger.git#1.0.2",
    "winston": "3.1.0",
    "winston-daily-rotate-file": "3.10.0",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "nodemon": "2.0.2",
    "pre-commit": "^1.2.2",
    "standard": "14.3.3"
  }
}
